{
  "name": "ascribe-react-components",
  "version": "0.0.1",
  "description": "Grab bag of useful react components when building react apps",
  "homepage": "https://github.com/ascribe/react-components#readme",
  "bugs": "https://github.com/ascribe/react-components/issues",
  "license": "Apache-2.0",
  "author": "Ascribe",
  "files": [
    "*.md",
    "bundle",
    "es6",
    "lib"
  ],
  "main": "lib/index.js",
  "jsnext:main": "es6/index.js",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascribe/react-components.git"
  },
  "private": true,
  "scripts": {
    "lint": "eslint ./",
    "postinstall": "npm run build",
    "build": "npm run build:bundle && npm run build:cjs && npm run build:es6",
    "build:bundle": "rimraf ./bundle && cross-env NODE_ENV=production webpack -p",
    "build:dev-extract": "cross-env NODE_ENV=extract webpack",
    "build:cjs": "npm run babel:cjs",
    "build:es6": "npm run babel:es6",
    "babel:cjs": "rimraf ./lib && cross-env NODE_ENV=production BABEL_ENV=cjs babel ./modules -d lib",
    "babel:es6": "rimraf ./es6 && cross-env NODE_ENV=production babel ./modules -d es6",
    "clean": "rimraf ./build ./bundle ./es6 ./lib",
    "start": "cross-env NODE_ENV=demo node server.demo.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.12.0",
    "eslint-config-ascribe-react": "^1.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.13",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.5",
    "core-js": "^2.4.0",
    "js-utility-belt": "^1.2.2",
    "moment": "^2.13.0",
    "react-css-modules": "^3.7.6",
    "react-datepicker": "^0.27.0",
    "react-textarea-autosize": "^4.0.2",
    "shallow-equals": "0.0.0"
  },
  "peerDependencies": {
    "react": "~0.14.0 || ^15.0.0",
    "react-addons-update": "~0.14.0 || ^15.0.0"
  },
  "keywords": [
    "react",
    "components",
    "fineuploader",
    "file",
    "files",
    "form",
    "input",
    "inputs",
    "uploader"
  ]
}
