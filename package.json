{
  "name": "ascribe-react-components",
  "version": "0.0.1",
  "description": "Grab bag of useful react components when building react apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascribe/react-components.git"
  },
  "homepage": "https://github.com/ascribe/react-components#readme",
  "bugs": "https://github.com/ascribe/react-components/issues",
  "main": "lib/index.js",
  "jsnext:main": "es6/index.js",
  "scripts": {
    "lint": "eslint ./",
    "postinstall": "npm run build",
    "build": "npm run build:main && npm run build:es6",
    "build:main": "babel ./modules -d ./lib --presets es2015",
    "build:es6": "babel ./modules -d ./es6 --presets es2015-no-commonjs"
  },
  "author": "Ascribe",
  "license": "Copyright",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "react": "~0.13.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.5.0"
  },
  "peerDependencies": {
    "react": "~0.13.0"
  },
  "keywords": [
    "react",
    "components",
    "fineuploader",
    "uploader"
  ]
}
