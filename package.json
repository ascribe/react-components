{
  "name": "ascribe-react-components",
  "version": "0.0.1",
  "description": "Grab bag of useful react components when building react apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascribe/react-components.git"
  },
  "homepage": "https://github.com/ascribe/react-components#readme",
  "bugs": "https://github.com/ascribe/react-components/issues",
  "main": "dist/bundle.js",
  "jsnext:main": "es6/index.js",
  "scripts": {
    "lint": "eslint ./",
    "postinstall": "npm run build",
    "build": "npm run build:main && npm run build:es6 && npm run build:dist",
    "build:main": "rimraf ./lib && babel ./modules -d ./lib --presets es2015",
    "build:es6": "rimraf ./es6 && babel ./modules -d ./es6 --presets es2015-no-commonjs",
    "build:dist": "rimraf ./dist && NODE_ENV=production webpack -p",
    "build:extract": "NODE_ENV=extract webpack",
    "clean": "rimraf ./build ./dist ./es6 ./lib",
    "start": "NODE_ENV=demo node server.demo.js"
  },
  "author": "Ascribe",
  "license": "Copyright",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "react": "^0.14.7",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-transform-hmr": "^1.0.2",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.0.7-beta",
    "webpack-combine-loaders": "^1.0.0",
    "webpack-dev-server": "^2.0.0-beta"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "bootstrap-css": "^3.0.0",
    "react-addons-update": "~0.14.0",
    "react-css-modules": "^3.7.4",
    "shallow-equals": "0.0.0"
  },
  "peerDependencies": {
    "babel-polyfill": "*",
    "react": "~0.14.0"
  },
  "keywords": [
    "react",
    "components",
    "fineuploader",
    "uploader"
  ]
}
